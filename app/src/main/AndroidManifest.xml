<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jsand.jsanders22ex">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ViewPagerActivity" />
        <activity android:name=".activity.LaunchModeActivity" />
        <activity
            android:name=".activity.StandardActivity"
            android:launchMode="standard" />
        <activity
            android:name=".activity.SingleInstanceActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name=".activity.SingleTaskActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".activity.SingleTopActivity"
            android:launchMode="singleTop" />
        <activity android:name=".activity.NinePatchActivity" />
        <activity android:name=".activity.ScaleTypeActivity" />
        <activity android:name=".activity.IntentAndBundleActivity" />
        <activity android:name=".activity.NotificationActivity" />
        <activity android:name=".activity.AdvancedListViewActivity"></activity>
    </application>

</manifest>